{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {
        "DEFAULT_MINIMUM_TOKENS": ""
      }
    },
    "youtube": {
      "command": "npx",
      "args": [
        "-y",
        "@anaisbetts/mcp-installer",
        "@anaisbetts/mcp-youtube"
      ]
    },
    "sequentialthinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "alwaysAllow": [
        "sequentialthinking"
      ]
    },
    "oxylabs": {
      "command": "uvx",
      "args": [
        "oxylabs-mcp"
      ],
      "env": {
        "OXYLABS_USERNAME": "hperkins_56bGN",
        "OXYLABS_PASSWORD": "Twiohmld1234+"
      }
    },
    "brave-search": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-brave-search"
      ],
      "env": {
        "BRAVE_API_KEY": "BSAWBhyyBo2etrHn20-CTj4xplPEx7-"
      }
    },
    "cloudflare": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://browser.mcp.cloudflare.com/sse"
      ],
      "disabled": true,
      "alwaysAllow": []
    },
    "playwright": {
      "command": "npx",
      "args": [
        "-y",
        "@playwright/mcp@latest",
        "--browser=",
        "--headless=",
        "--viewport-size="
      ],
      "disabled": true,
      "alwaysAllow": []
    },
    "puppeteer": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-puppeteer"
      ],
      "alwaysAllow": []
    }
  }
}