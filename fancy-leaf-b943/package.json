{
	"name": "fancy-leaf-b943",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"dev:worker": "wrangler dev --config wrangler.jsonc --port 8788 --inspector-port 9330",
		"dev:server": "tsx watch server.ts",
		"dev:all": "concurrently -k -p \"[{name}]\" -n \"Vite,Worker,Server\" -c \"cyan,yellow,magenta\" \"npm run dev\" \"npm run dev:worker\" \"npm run dev:server\"",
		"build": "tsc -b && vite build",
		"build:server": "tsc server.ts --outDir dist-server",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"typecheck": "tsc -b --noEmit",
		"format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
		"format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,css,md}\"",
		"preview": "npm run build && vite preview",
		"deploy": "npm run build && wrangler deploy --config wrangler.jsonc",
		"deploy:prod": "npm run build && wrangler deploy --config wrangler.jsonc --env production",
		"cf-typegen": "wrangler types",
		"clean": "rm -rf dist node_modules/.vite dist-server",
		"test": "vitest",
		"test:watch": "vitest watch",
		"test:coverage": "vitest run --coverage",
		"d1:migrate:local": "wrangler d1 execute claude-code --file=./migrations/0001_create_tables.sql --local",
		"d1:migrate:remote": "wrangler d1 execute claude-code --file=./migrations/0001_create_tables.sql --remote"
	},
	"dependencies": {
		"@anthropic-ai/claude-code": "^1.0.70",
		"@anthropic-ai/sdk": "^0.59.0",
		"@browserbasehq/stagehand": "^2.4.2",
		"@monaco-editor/react": "^4.7.0",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.21.2",
		"lucide-react": "^0.536.0",
		"puppeteer": "^24.16.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-resizable-panels": "^3.0.4",
		"react-router-dom": "^6.27.0",
		"react-window": "^1.8.9",
		"sonner": "^2.0.7",
		"zustand": "^5.0.7"
	},
	"devDependencies": {
		"@cloudflare/vite-plugin": "^1.11.1",
		"@eslint/js": "^9.30.1",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/postcss": "^4.1.11",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.11",
		"@testing-library/jest-dom": "^6.6.4",
		"@testing-library/react": "^16.1.0",
		"@testing-library/user-event": "^14.5.2",
		"@types/cors": "^2.8.17",
		"@types/express": "^5.0.0",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"@vitejs/plugin-react-swc": "^3.10.2",
		"@vitest/coverage-v8": "^2.1.8",
		"autoprefixer": "^10.4.21",
		"concurrently": "^9.1.2",
		"eslint": "^9.30.1",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.20",
		"globals": "^16.3.0",
		"jsdom": "^25.0.1",
		"postcss": "^8.5.6",
		"prettier": "^3.4.2",
		"tailwindcss": "^4.1.11",
		"tsx": "^4.19.2",
		"typescript": "~5.8.3",
		"typescript-eslint": "^8.35.1",
		"vite": "^7.0.4",
		"vite-bundle-visualizer": "^1.2.1",
		"vitest": "^2.1.8",
		"wrangler": "^4.28.1"
	}
}
